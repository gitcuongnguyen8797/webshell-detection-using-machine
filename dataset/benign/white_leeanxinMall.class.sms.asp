Private Function UrlEncode_GBToUtf8_V2(szInput)
Dim wch, uch, szRet
Dim x
Dim nAsc, nAsc2, nAsc3
If szInput = "" Then
UrlEncode_GBToUtf8_V2= szInput
Exit Function
End If
For x = 1 To Len(szInput)
wch = Mid(szInput, x, 1)
nAsc = AscW(wch)
If nAsc < 0 Then nAsc = nAsc + 65536
If wch = OOOOOO0O(ChrW(90)) then
szRet = szRet & OOOOOO0O(ChrW(84)&ChrW(97)&ChrW(113))
ElseIf wch = OOOOOO0O(ChrW(84)) then
szRet = szRet & OOOOOO0O(ChrW(84)&ChrW(97)&ChrW(100))
ElseIf (nAsc And &HFF80) = 0 Then
szRet = szRet & wch
Else
If (nAsc And &HF000) = 0 Then
uch = OOOOOO0O(ChrW(84)) & Hex(((nAsc \ 2 ^ 6)) Or &HC0) & Hex(nAsc And &H3F Or &H80)
szRet = szRet & uch
Else
uch = OOOOOO0O(ChrW(84)) & Hex((nAsc \ 2 ^ 12) Or &HE0) & OOOOOO0O(ChrW(84)) & _
Hex((nAsc \ 2 ^ 6) And &H3F Or &H80) & OOOOOO0O(ChrW(84)) & _
Hex(nAsc And &H3F Or &H80)
szRet = szRet & uch
End If
End If
Next
UrlEncode_GBToUtf8_V2= szRet
End Function
End Class
