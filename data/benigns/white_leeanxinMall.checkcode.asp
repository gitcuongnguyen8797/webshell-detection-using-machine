Response.AddHeader OOOOOO0O(ChrW(114)&ChrW(64)&ChrW(63)&ChrW(69)&ChrW(54)&ChrW(63)&ChrW(69)&ChrW(92)&ChrW(115)&ChrW(58)&ChrW(68)&ChrW(65)&ChrW(64)&ChrW(68)&ChrW(58)&ChrW(69)&ChrW(58)&ChrW(64)&ChrW(63)), OOOOOO0O(ChrW(58)&ChrW(63)&ChrW(61)&ChrW(58)&ChrW(63)&ChrW(54)&ChrW(106)&ChrW(32)&ChrW(44)&ChrW(55)&ChrW(58)&ChrW(61)&ChrW(54)&ChrW(63)&ChrW(50)&ChrW(62)&ChrW(54)&ChrW(108)&ChrW(52)&ChrW(50)&ChrW(65)&ChrW(69)&ChrW(52)&ChrW(57)&ChrW(50)&ChrW(93)&ChrW(51)&ChrW(62)&ChrW(65))
Response.CacheControl = OOOOOO0O(ChrW(63)&ChrW(64)&ChrW(92)&ChrW(52)&ChrW(50)&ChrW(52)&ChrW(57)&ChrW(54))
Response.AddHeader OOOOOO0O(ChrW(33)&ChrW(67)&ChrW(50)&ChrW(56)&ChrW(62)&ChrW(50)), OOOOOO0O(ChrW(63)&ChrW(64)&ChrW(92)&ChrW(52)&ChrW(50)&ChrW(52)&ChrW(57)&ChrW(54))
Response.Expires = -1
SendHex(Join(BmpHeader,""))
SendHex(Join(BmpInfoHeader,""))
SendHex(BmpColorMap)
for y=ImageHeight to 1 step -1
for x=1 to ImageWidth
tmpHex = cfg_codebitmap(y,x)
if tmpHex = vbNullString then
SendHex(BColor)
else
SendHex(tmpHex)
end if
next
SendHex(BmpEndLine)
next
Response.Flush
end sub
